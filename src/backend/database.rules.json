{
  /* Visit https://firebase.google.com/docs/database/security to learn more about security rules. */
  "rules": {
    // ".read": "now < 1702789200000",  // 2023-12-17
    // ".write": "now < 1702789200000",  // 2023-12-17
    "users": {
      "$uid": {
        ".read": "auth.uid !== null",
        ".write": "auth.uid === $uid"
      }
    },
    "patients": {
      ".read": "auth.uid !== null",
      ".write": "auth.uid !== null"
    },
    "scans": {
      ".read": "auth.uid !== null",
      ".write": "auth.uid !== null"
    }
  }
}